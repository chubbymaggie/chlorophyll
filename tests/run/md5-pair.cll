int::2 leftrotate(int::2 x, int r) {
  return (x << r) || (x >> (32 - r));
}

int::2 func_f(int::2 b, int::2 c, int::2 d) {
  return (b && c) || (!b && d);
}


int::2 sumrotate(int::2 a, int::2 b, int::2 f, int::2 k, int::2 m, int r) {
  return b + leftrotate(a + f + k + m, r);
}

void main() {
  int::2[] k[32]; // full = 64 entries
  int[] r[32];
  int::2[] message[16];
  
  int::2 a, b, c, d, temp;
  int g;
  
  for(t from 0 to 16) {
    for(i from 0 to 32) { 
      temp = sumrotate(a,b,func_f(b,c,d),k[i],message[g],r[i]);
    }
  } 
}
