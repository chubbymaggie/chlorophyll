
int@1->int@2 filter First() {
  int@1 x;
  int@2 y;

  work blocking {
    y = in();
    x = y;
    out(x + 5);
  }
}

int@3->int@4 filter Second() {
  int@3 x;
  int@4 y;

  work blocking {
    x = in();
    y = x;
    out(x - 2);
  }
}

int@5->int@7 filter Third() {
  int@6 x;
  int@5 y;
  y = 2;

  work blocking {
    x = (in() + 7)*y;
    out(x);
  }
}

int->int pipeline Main() {
  add First();
  add Second();
  add Third();
}

