int::2@(28, 29) leftrotate(int::2@(17,17) x, int@17 r) {
  int::2@(28, 29) out;
  int@17 swap;
  int@34 r2;
  int::2@(34,34) x2;
  int@35 r3;
  int::2@(35,35) x3;
  if(r >@17 16) {
    swap = x::0;
    x::0 = x::1;
    x::1 = swap;
    r = r -@17 16;
  }
  x2 = x;
  x3 = x;
  r2 = r;
  r3 = r;
  out::0 = ((x3::1 >>@35 (16 -@35 r3)) |@34 (x2::0 <<@34 r2)) &@34 65535;
  out::1 = ((x3::0 >>@35 (16 -@35 r3)) |@34 (x2::1 <<@34 r2)) &@34 65535;
  return out;
}

int::2@(28, 29) csum(int::2@(28, 29) x, int::2@(28, 29) y) {
  int::2@(28, 29) z;
  z::0 = x::0 +@28 y::0;
  z::1 = x::1 +@29 y::1 +@29 ((z::0 >>@28 16) &@29 3);
  z::0 = z::0 &@28 65535;
  z::1 = z::1 &@29 65535;
  return z;
}

int::2@(24,24) cadd(int::2@(24,24) x, int::2@(24,24) y) {
  int::2@(24,24) z;
  z::0 = x::0 +@24 y::0;
  z::1 = x::1 +@24 y::1 +@24 ((z::0 >>@24 16) &@24 3);
  z::0 = z::0 &@24 65535;
  z::1 = z::1 &@24 65535;
  return z;
}

