int::2@(28, 29) leftrotate(int::2@(17,17) x, int@17 r) {
  int::2@(28, 29) dummy;
  int@17 swap;
  int@34 r2;
  int::2@(34,34) x2;
  int@35 r3;
  int::2@(35,35) x3;
  if(r >@17 16) {
    swap = x::0;
    x::0 = x::1;
    x::1 = swap;
    r = r -@17 16;
  }
  x2 = x;
  x3 = x;
  //out(r);
  r2 = r;
  r3 = r;
  dummy::0 = ((x3::1 >>@35 (16 -@35 r3)) |@34 (x2::0 <<@34 r2)) &@34 65535;
  dummy::1 = ((x3::0 >>@35 (16 -@35 r3)) |@34 (x2::1 <<@34 r2)) &@34 65535;
  //out(dummy::0);
  //out(dummy::1);
  return dummy;
}

void main() {
  int::2 x, res;
  x::0 = 10;
  x::1 = 20;
  res = leftrotate(x, 5);
  //out(res::0);
  //out(res::1);
}
