void main() {
  int@0 x; //(-2**16, 2**16)
  int@1 y;
  int@1 cos;
  int[]@{[0:5]=1} coeff[5] = {2730, 0, -32768, 0, 65536};

  x = in(); // x in revolutions
  if(x > 32768) { // 2**15
    x = 65536 - x; // 2**16
  }
  if(x > 16384) {
    y = 32768 - x;
  } else {
    y = x;
  }

  y = (y << 2) */16 102943; // y*3.142...

  cos = 0;
  for(i from 0 to 5) {
    cos = cos */16 y + coeff[i];
  }

  if(x > 16384) {
    cos = -cos;
  }
  out(cos);
}
