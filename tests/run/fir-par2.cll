void main() {
  int[]@{[0:4]=0} filter0[4] = {1,4,3,2};
  int[]@{[0:4]=1} filter1[4] = {5,8,7,6};
  int[]@{[0:4]=2} filter2[4] = {9,12,11,10};
  int[]@{[0:4]=3} filter3[4] = {13,16,15,14};
  int[]@{[0:4]=0} x0[4] = {0 , 0 , 0 , 0};
  int[]@{[0:4]=1} x1[4] =  {0 , 0 , 0 , 0};
  int[]@{[0:4]=2} x2[4] =  {0 , 0 , 0 , 0};
  int[]@{[0:4]=3} x3[4] =  {0 , 0 , 0 , 0};

  int@0 sum0, b0;
  int@1 sum1, b1;
  int@2 sum2, b2;
  int@3 sum3, b3;
  int@15 offset; // offset 0
  
  b0 = offset;
  b1 = b0;
  b2 = b1;
  b3 = b2;
  x3[b3] = x2[b2];
  x2[b2] = x1[b1];
  x1[b1] = x0[b0];
  x0[b0] = in();
  sum0 = 0;
  sum1 = 0;
  sum2 = 0;
  sum3 = 0;
  
  for(i from 0 to 4) {
    sum0 = sum0 +@0 filter0[i] *@0 x0[b0];
    b0 = (b0 +@0 1) &@0 3;
  }

  for(i from 0 to 4) {
    sum1 = sum1 +@1 filter1[i] *@1 x1[b1];
    b1 = (b1 +@1 1) &@1 3;
  }

  for(i from 0 to 4) {
    sum2 = sum2 +@2 filter2[i] *@2 x2[b2];
    b2 = (b2 +@2 1) &@2 3;
  }

  for(i from 0 to 4) {
    sum3 = sum3 +@3 filter3[i] *@3 x3[b3];
    b3 = (b3 +@3 1) &@3 3;
  }

  offset = (offset +@place(offset) 1) &@place(offset) 3;
  out(sum3 +@2 sum2 +@1 sum1 +@0 sum0);
}
