void main() {
  int x; //(-2**16, 2**16)
  int y;
  int sin;
  int coeff[8] = {-13, 0, 546, 0, -10923, 0, 65536, 0};

  x = in(); // x in revolutions
  if(x > 32768) { // 2**15
    y = 65536 - x; // 2**16
  }

  y = (y << 2) */16 102943; // y*3.142...

  sin = 0;
  for(i from 0 to 8) {
    sin = sin */16 y + coeff[i];
  }

  if(x > 32768) {
    sin = -sin;
  }
  out(sin);
}
