int::2 cadd(int::2 a, int b) {
  int::2 z;
  z::0 = a::0 + b;
  z::1 = a::1 + (z::0 >> 16);
  z::0 = z::0 & 65535;
  return z;
}

void main() {
  int[] l[120];
  int[] r[120];
  int[] diff[120];
  int::2 sum;

  l[0] = in();

  for(i from 0 to 120) {
    diff[i] = (l[i] - r[i]) * (l[i] - r[i]);
  }

  for(i from 0 to 120) {
    sum = cadd(sum, diff[i]);
  }

  out(sum);
}
