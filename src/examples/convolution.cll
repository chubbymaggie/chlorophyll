void main() {
  int[] in[100];
  int[] out[100];
  int[] kernel[5]; // best to duplicate kernel on every core that has input/output
  
  for(i from 0 to 96) {
    for(j from 0 to 5) {
      out[i] = in[i + j] * kernel[j] +@place(out[i]) + out[i];
    }
  }

