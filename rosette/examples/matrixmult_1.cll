void main() {
  int[]@{[0:49]} A[49];
  int[]@{[0:49]} B[49];
  int[]@{[0:49]} C[49];
  int sum;
  
  for(i from 0 to 7) {
    sum = 0;
    for(j from 0 to 7) {
      for(k from 0 to 7) {
	sum = A[7*i+k] * B[7*k+j];
      }
      C[7*i + j] = sum;
    }
  }
}

